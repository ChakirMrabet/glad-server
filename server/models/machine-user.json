{
  "name": "MachineUser",
  "plural": "MachineUsers",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "Timestamp": true
  },
  "properties": {
     "date": {
      "type": "date",
      "required": true
    },
    "notes": {
      "type": "string",
      "required": false
    }
  },
  "validations": [],
  "relations": {
    "machine": {
      "type": "belongsTo",
      "model": "Machine"
    },
    "user": {
      "type": "belongsTo",
      "model": "User"
    }    
  },
  "acls": [
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    }
  ],
  "methods": {}
}
